{
  "private": true,
  "scripts": {
    "build-prod": "npm run build-docs && npm run build-css && npm run copy-css",
    "build-dev": "pulp build",
    "watch": "pulp -w build",
    "build-docs": "pulp build -I test --to docs/example.js",
    "build-css": "cd css && npm run build-prod",
    "copy-css": "cp css/dist/cn-tailwind.css docs/cn-tailwind.css"
  },
  "devDependencies": {
    "pulp": "^12.0.1",
    "purescript": "^0.11.6"
  }
}
